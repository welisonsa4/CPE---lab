#include <iostream>

int main() {
    int x, y, z;

    x = 7 + 3 * 6 / 2 - 1;
    y = 2 % 2 + 2 * 2 / 2;
    z = (3 * 9 * (3 + (9 * 3 / (3))));

    std::cout << "x = " << x << "\n";
    std::cout << "y = " << y << "\n";
    std::cout << "z = " << z << "\n";

    return 0;
}
